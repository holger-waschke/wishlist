{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Go: Launch Package (debug web server)",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "env": {
        "PORT": "5000",
        "GIN_MODE": "debug"
      },
      "args": [],
      "buildFlags": "",
      "cwd": "${workspaceFolder}",
      "showLog": true
    },
    {
      "name": "Go: Attach to Process (pick)",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": "${command:pickProcess}",
      "cwd": "${workspaceFolder}",
      "showLog": true
    },
    {
      "name": "Go: Remote Attach to headless Delve",
      "type": "go",
      "request": "attach",
      "mode": "remote",
      "remotePath": "",
      "port": 2345,
      "host": "127.0.0.1",
      "cwd": "${workspaceFolder}",
      "showLog": true
    }
  ]
}